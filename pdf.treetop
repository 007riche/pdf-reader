grammar Pdf
  rule doc
    ( string_literal / space / string_hex / float / integer / name)*
  end

  rule string_literal
    "(" [^()]+ ")" <PDF::Reader::LiteralString>
  end

  rule string_hex
    "<" [0-9A-Fa-f]+ ">" <PDF::Reader::HexString>
  end

  rule float
    [0-9]+ "." [0-9]+ <PDF::Reader::Float>
  end

  rule integer
    [0-9]+ <PDF::Reader::Integer>
  end

  rule name
    "/" [A-Za-z0-9]+ <PDF::Reader::Name>
  end

  rule space
    [\s]+
  end
end
