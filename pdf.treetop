grammar Pdf
  rule string_literal
    space? string_literal_start string_literal_body string_literal_end space?
  end

  rule string_literal_start
    "(" <PDF::Reader::Token>
  end

  rule string_literal_end
    ")" <PDF::Reader::Token>
  end

  rule string_literal_body
    [a-z]+ <PDF::Reader::Token>
  end

  rule space
    [\s]+
  end
end
